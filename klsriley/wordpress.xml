	<item>
		<title>Creativity - Implications for the Classroom</title>
		<link>http://www.klsriley.com/2016/08/29/creativity-implications-for-the-classroom/</link>
		<pubDate>Tue, 30 Aug 2016 03:13:32 +0000</pubDate>
		<dc:creator><![CDATA[klsriley]]></dc:creator>
		<guid isPermaLink="false">http://www.klsriley.com/?p=1824</guid>
